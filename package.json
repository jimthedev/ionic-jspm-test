{
  "name": "my-app",
  "version": "1.0.0",
  "description": "my-app: An Ionic project",
  "dependencies": {
    "gulp": "^3.5.6",
    "gulp-sass": "^1.3.3",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0"
  },
  "devDependencies": {
    "bower": "^1.3.3",
    "gulp-util": "^2.2.14",
    "jspm": "^0.16.6",
    "shelljs": "^0.3.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "com.ionic.keyboard"
  ],
  "cordovaPlatforms": [],
  "jspm": {
    "map": {
      "angular": "ionic/angular"
    },
    "dependencies": {
      "ionic": "github:driftyco/ionic-bower@^1.1.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    },
    "overrides": {
      "github:driftyco/ionic-bower@1.1.0": {
        "main": "js/ionic-angular",
        "shim": {
          "js/ionic-angular": {
            "deps": [
              "./ionic",
              "angular",
              "angular-animate",
              "angular-sanitize",
              "angular-ui-router"
            ]
          }
        }
      }
    }
  }
}
